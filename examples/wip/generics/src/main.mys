def double(value: i32) -> i32:
    return 2 * value

def double(value: string) -> i32:
    return double(i32(value))

@generic(T)
def triple(value: T) -> T:
    return 2 * value

@generic(T)
class Adder:

    value: T

    def add(self, value: T):
        self.value += value

def main():
    print(double(5))
    print(double("1"))

    print(triple<u8>(5))
    print(triple(5))
    print(triple(5.5))

    adder = Adder<i32>()
    adder.add(3)
    print(adder.value)
