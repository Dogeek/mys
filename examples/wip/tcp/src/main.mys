from .server import Server
from .client import Client

ADDRESS: str = '127.0.0.1:3232'

def main(args: [str]):
    command = args[1]

    if command == 'server':
        server = Server(ADDRESS)
        server.start()
    elif command == 'client':
        client = Client(ADDRESS)
        client.start()
    else:
        raise ValueError(command)
