@test
def test___line__():
    x: u64 = __line__
    y = __line__
    z = __line__ + __line__

    assert x == 3
    assert y == 4
    assert z == 2 * 5

@test
def test___unique_id__():
    x: i64 = __unique_id__
    y = __unique_id__

    assert x != y

@test
def test___name__():
    assert __name__ == "test_all.special_symbols"

@test
def test___file__():
    assert __file__ == "./src/special_symbols.mys"
