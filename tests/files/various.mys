def main():
    # Text.
    with open('foo.txt', 'w') as fout:
        fout.write('1')

    with open('foo.txt', 'r') as fin:
        assert fin.read() == '1'

    # Binary.
    with open('foo.bin', 'wb') as fout:
        fout.write(b'\x01')

    with open('foo.bin', 'rb') as fin:
        assert fin.read() == b'\x01'

    if True:
        print(1)

    if False:
        print(2)
    else:
        print(3)

    if True:
        print(4)
    elif False:
        print(5)
    elif True:
        print(6)
    else:
        print(7)

    try:
        raise TypeError()
    except ValueError:
        print('ValueError')
    except TypeError:
        print('TypeError')
    finally:
        print('finally')

    try:
        raise ValueError()
    except ValueError:
        print('ValueError')
        raise
