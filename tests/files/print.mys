@trait
class Base:
    pass

class Foo(Base):
    v: i32

class Bar(Base):
    a: Foo
    b: bool
    c: string

class Fie:
    a: i16
    _b: bool
    _c: string

class Fam:
    x: Base

@enum
class Animal:
    Ape = 0
    Cow = 1

def print_base(value: Base):
    print(value)

def main():
    print("A string literal!")
    print(1)
    print(1.5)
    print(False)
    print(True)
    print(str(Foo(5)))
    print((-500, "Hi!"))
    print([1, 2, 3])
    print(Bar(Foo(3), True, "kalle"))
    print(Foo(5))
    print([(Foo(3), True), (Foo(5), False)])
    v = True
    print(v)
    print(1 == 2)
    print(v in [True])
    l: [bool] = None
    print(l)
    print(Fie(5))
    print('G')
    print(char(0x37))
    print(['j', 'u', 'l'])
    print(Fam(None))
    print(Fam(Foo(4)))
    print(Fam(Bar(None, False, "kk")))
    print_base(Foo(-1))
    print_base(Bar(None, True, ""))
    l2: [Base] = [Foo(5), Bar(None, True, "fes")]
    print(l2)
    print(b"")
    print(b"\x01\x02\x03", end="", flush=True)
    print("", end="!\n")
    print(Animal.Cow)
    print({1: 2, 3: 4})
    print({"hi": Foo(5), "ho": Foo(4)})
