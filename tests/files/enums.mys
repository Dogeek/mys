@enum(u8)
class Color:

    Red = 0
    Green = 1
    Blue = 2

@enum
class Color2:

    Red = -10000000000000
    Green = 0
    Blue = 1

@test
def test_all():
    # assert_eq(Color(0), Color.Red)
    assert_eq(1, Color.Green)
    assert_eq(Color.Red + 2, Color.Blue)
    assert_eq(Color2.Red, -10000000000000)
    assert_eq(Color2.Green, 0)
    assert_eq(Color2.Blue, 1)
